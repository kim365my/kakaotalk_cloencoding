				act'끝마친다':mirror_steam = hour & gt $loc
			end
		end
	end
end


if $ARGS[0] = 'ass_compliment':
	cla & *clr
	inhib_exp += 1
	'<center><video autoplay loop src="images/shared/home/bathroom/ass_compliment.mp4"></center>'
	'당신이 머리카락에 샴푸 칠을 할 때, 남동생이 뭔가에 대해 말하기로 결심한다.'
	'"저기, <<$pcs_nickname>>. 뭐 좀 말해도 돼?"'
	'"흐음? 물론이지, 뭔데 콜카?"'
	'"너 진짜 끝내주는 엉덩이를 가졌네!"'
	'안 봐도 분명히 미소짓고 있을 그를 느끼면서, 당신은 그의 칭찬에 대한 성의를 보여주기 위해 어깨너머로 그를 돌아보며 히죽 웃고 엉덩이를 약간 흔들어 보인다.'
	act '계속':gt'brother_voyeur','shave'
end

if $ARGS[0] = 'shave':
	if pcs_pubes < 4 and pcs_leghair < 4: gt'brother_voyeur', 'dryoff'
	cla & *clr
	if brothershowerlook = 1:
		'<center><img <<$set_imgh>> src="images/shared/home/bathroom/brothertalk4.jpg"></center>'
		'당신이 몸을 헹궈내다가 콜카가 당신의 다리와 그 사이의 무언가를 빤히 보고 있는 것을 눈치챈다.'
		'"저기?" 당신은 그에게 날이 선 목소리로 묻는다. "뭐 좀 도와줄까?"'
		if pcs_pubes < 11 and pcs_leghair > 3:
			'그가 입술을 우물거리며 생각하더니, "네 그 아래가 많이 자란 것 같은데."하고 말한다. 당신이 그의 시선을 따라가자 <<$pc_desc[''legs'']>> 다리와 보지가 보인다. "너 면도 안 할 거야?"'
		elseif pcs_pubes >= 11 and pcs_pubes < 21 and pcs_leghair > 3:
			'그가 입술을 우물거리며 생각하더니,"네 그 아래쪽에서 덤불이 자라고 있어."하고 말한다. 당신이 그의 시선을 따라가자 당신의 <<$pc_desc[''legs'']>> 다리와 당신의 다듬어진 보지 털이 보인다. "네 다리 좀 봐. 마치 잔디를 기르고 있는 것 같잖아. 너 면도 안 할 거야?"'
		elseif pcs_pubes >= 21 and pcs_leghair > 3:
			'그가 입술을 우물거리며 생각하더니, "네 그 아래쪽 덤불이 이제는 좀 더 숲처럼 보이잖아."하고 말한다. 당신이 그의 시선을 따라가자 당신의 <<$pc_desc[''legs'']>> 다리와 보지 위를 뒤덮는 빽빽한 음모가 보인다. "게다가 네 다리 좀 봐. 마치 잔디를 기르고 있는 것 같잖아. 너 면도 안 할 거야?"'
